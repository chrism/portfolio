<h1 class="main-heading"><%= @project.title %></h1>
<div class="images">
	<% if @project.projectimages %>		
		<ul>
			<% @project.projectimages.each do |projectimage| %>
		    <li><%= image_tag projectimage.url %></li>
			<% end %>
		</ul>
	<% end %>
</div>
<p>
  <b class="description">Descqsqsronsssss:</b>
  <%= @project.description %>
</p>

<% if can? :update, Project %>
	<%= link_to 'Edit', edit_project_path(@project) %> |
<% end %>
<%= link_to 'Back', projects_path %>
